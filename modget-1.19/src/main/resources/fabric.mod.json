    "authors": [
        "NebelNidas",
        "pozitp",
        "TheEnderOfficial",
        "TheBrokenRail",
        "gbl",
        "Samekichi"
    ],
    "contact": {
        "homepage": "https://github.com/ReviversMC/modget-minecraft",
        "sources": "https://github.com/ReviversMC/modget-minecraft",
        "issues": "https://github.com/ReviversMC/modget-minecraft/issues"
    },
    "icon": "assets/modget/icon.png",
