{
    "schemaVersion": 1,
    "id": "modget",
    "version": "${version}",
    "name": "ModGet",
    "description": "Minecraft Mod Package Manager! Searches for updates, installs them etc.",
    "authors": [
        "NebelNidas"
    ],
    "contact": {
        "homepage": "https://github.com/ReviversMC/modget",
        "sources": "https://github.com/ReviversMC/modget",
        "issues": "https://github.com/ReviversMC/modget/issues"
    },
    "icon": "assets/modget/icon.png",
    "license": "GPL3",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.nebelnidas.modget.Modget"
        ]
    },
    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric": "*",
        "minecraft": ["1.16.4", "1.16.5"]
    },
    "custom": {
        "modupdater": {
            "strategy": "curseforge",
            "projectID": 391583
        }
    }
}
