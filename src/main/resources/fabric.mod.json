{
    "schemaVersion": 1,
    "id": "modget",
    "version": "${version}",
    "name": "ModGet",
    "description": "Minecraft Mod Package Manager! Searches for updates, installs them etc.",
    "authors": [
        "NebelNidas"
    ],
    "contact": {
        "homepage": "https://github.com/ReviversMC/modget",
        "sources": "https://github.com/ReviversMC/modget",
        "issues": "https://github.com/ReviversMC/modget/issues"
    },
    "icon": "assets/modupdater/icon.png",
    "license": "GPL3",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.thebrokenrail.modupdater.ModUpdater"
        ],
        "modmenu": [
            "com.thebrokenrail.modupdater.client.ModMenu"
        ]
    },
    "depends": {
        "fabricloader": ">=0.7.4",
        "fabric": "*",
        "minecraft": "1.16.x"
    },
    "suggests": {
        "modmenu": "*"
    },
    "custom": {
        "modupdater": {
            "strategy": "curseforge",
            "projectID": 391583
        }
